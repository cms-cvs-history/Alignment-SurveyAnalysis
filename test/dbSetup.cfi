include "CondCore/DBCommon/data/CondDBSetup.cfi"

block dbSetup =
{
  using CondDBSetup

  string connect  = "sqlite_file:../Alignments.db"    
  untracked string timetype = "runnumber"    
}

block surveyValueRcd = { string record = "TrackerSurveyRcd"      string tag = "valueTag" }
block surveyErrorRcd = { string record = "TrackerSurveyErrorRcd" string tag = "errorTag" }
block alignValueRcd = { string record = 'TrackerAlignmentRcd'      string tag = "alignValue" }
block alignErrorRcd = { string record = 'TrackerAlignmentErrorRcd' string tag = "alignError" }
